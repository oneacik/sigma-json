{
  "name": "mobx-react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with Typescript, ES6 compilation and hot code reloading",
  "scripts": {
    "start": "node express.js",
    "build": "webpack",
    "dev": "node server.js",
    "prettify": "prettier --config .prettierrc.json -w 'src/**/*.{ts,tsx}'",
    "eslintify": "eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "test": "jest"
  },
  "engines": {
    "node": "v13.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettify"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-typescript-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "html-webpack-plugin": "^4.4.1",
    "ts-essentials": "^7.0.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.13",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.161",
    "@types/object-hash": "^1.3.3",
    "@types/react-dom": "^16.9.8",
    "@types/sinonjs__fake-timers": "^6.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "express": "^4.17.1",
    "fileapi": "^2.2.0",
    "hashset": "0.0.6",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "jsonpath": "^1.0.2",
    "lodash": "^4.17.20",
    "lolex": "^6.0.0",
    "mobx": "^5.15.6",
    "mobx-react": "^6.3.0",
    "object-hash": "^2.0.3",
    "prettier": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0"
  }
}
